<?xml version="1.0" encoding="utf-8"?>
<org.ninetripods.mq.study.NestedScroll.util.view.NestedParentView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.ninetripods.mq.study.NestedScroll.nested.NestedScrollActivity">

    <ImageView
        android:id="@id/id_nested_layout_top"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/xiangyu" />

    <org.ninetripods.mq.study.NestedScroll.util.view.SimpleViewPagerIndicator
        android:id="@id/id_nested_layout_indicator"
        android:layout_width="match_parent"
        android:layout_height="50dp" />

    <android.support.v4.view.ViewPager
        android:id="@id/id_nested_layout_viewpager"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</org.ninetripods.mq.study.NestedScroll.util.view.NestedParentView>
